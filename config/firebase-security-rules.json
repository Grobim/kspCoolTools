{
  "rules": {
    ".read": false,
    ".write": false,
    "profiles": {
      ".read": true,
      "$profile_id": {
        ".write": "auth != null && $profile_id === auth.uid"
      }
    },
    "profilePrivateInfos": {
      "$profilePrivateInfo_id": {
        ".read": "auth != null && $profilePrivateInfo_id === auth.uid",
        ".write": "auth != null && $profilePrivateInfo_id === auth.uid"
      }
    },
    "saveManager": {
      ".read": true,
      ".write": "auth != null"
    }
  }
}
